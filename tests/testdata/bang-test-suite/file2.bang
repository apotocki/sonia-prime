assert(xpack0.0 == 1);
assert(xpack0[0] == 1);
assert(xpack0.1 == "345");
assert(xpack0[1] == "345");
assert(size(xpack0.name) == 3);
//assert(size(xpack0["name"]) == 3);
assert(size(xpack0[.name]) == 3);
